[Appearance]
AntiAliasFonts=true
ColorScheme=Wookie Bright
Font=Liberation Mono,11,-1,5,50,0,0,0,0,0

[Cursor Options]
CursorShape=1

[General]
Directory=$HOME
Environment=TERM=xterm,SHELL=/bin/zsh
LocalTabTitleFormat=%w
Name=Wookie
Parent=FALLBACK/
RemoteTabTitleFormat=(%h) %w

[Interaction Options]
AutoCopySelectedText=false
TrimTrailingSpacesInSelectedText=true

[Keyboard]
KeyBindings=default

[Terminal Features]
BlinkingCursorEnabled=true
